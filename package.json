    "author": "Mac Gainor",
    "bugs": {
        "url": "https://github.com/mac-s-g/react-json-view/issues"
    },
    "homepage": "https://github.com/mac-s-g/react-json-view",
    "directories": {
        "docs": "docs",
        "test": "test"
    }
