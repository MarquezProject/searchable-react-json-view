{
    "name": "searchable-react-json-view",
    "description": "Interactive react component for displaying javascript arrays and JSON objects.",
    "version": "0.0.8",
    "main": "dist/main.js",
    "dependencies": {
        "flux": "^4.0.1",
        "react-base16-styling": "^0.6.0",
